pipeline {
    agent { 
		docker { 
			image 'maven:3.3.3' 
        		args '--network training-playground'
		}
	}
    stages {
        stage('tests') {
            steps {
                sh "mvn -f '005 - Recording Test Results and Artifacts/03 - Sonar Integration' sonar:sonar -Dsonar.projectKey='test-moises' \
		-Dsonar.projectName='test-moises' -Dsonar.host.url=http://sonarqube:8081"
            }
        }
    }
}
