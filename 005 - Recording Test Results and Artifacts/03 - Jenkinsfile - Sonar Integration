pipeline {
    agent { 
		docker { 
			image 'maven:3.3.3' 
		}
	}
    stages {
        stage('tests') {
            steps {
                sh "mvn -Dsonar.projectKey='test-moises' \
		-Dsonar.projectName='test-moises' \ 
		-f '005 - Recording Test Results and Artifacts/03 - Sonar Integration' sonar:sonar -Dsonar.host.url=http://3.124.164.63:8081"
            }
        }
    }
}
