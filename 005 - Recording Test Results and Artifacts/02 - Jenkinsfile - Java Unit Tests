pipeline {
    agent { 
		docker { 
			image 'maven:3.3.3' 
		}
	}
    stages {
        stage('tests') {
        steps {
        	configFileProvider(
        		[configFile(fileId: 'settings.xml',
        			variable: 'MAVEN_SETTINGS')]) {
        			sh 'mvn -s $MAVEN_SETTINGS -f 005 - Recording Test Results and Artifacts/02 - Java Unit Tests/pom.xml clean install'
                }
        }
	    }
  }
}
