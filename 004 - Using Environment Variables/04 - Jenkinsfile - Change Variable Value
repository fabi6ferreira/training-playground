pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
   	    stage ('teste') {
	    environment {
			NUMBER_OF_ISSUES = "test_notification"
		    }
		    steps {
		    	script {
				    env.NUMBER_OF_ISSUES = "10"
				    echo '$NUMBER_OF_ISSUES'
				}
			}
	    }
	}
}
